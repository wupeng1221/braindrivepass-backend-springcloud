<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wup.mapper.QuestionMapper">

    <resultMap id="BaseResultMap" type="com.wup.entity.Question">
            <id property="id" column="id" jdbcType="BIGINT"/>
            <result property="orderId" column="order_id" jdbcType="INTEGER"/>
            <result property="code" column="code" jdbcType="VARCHAR"/>
            <result property="type" column="type" jdbcType="TINYINT"/>
            <result property="question" column="question" jdbcType="VARCHAR"/>
            <result property="selectItem" column="select_item" jdbcType="VARCHAR"/>
            <result property="itemA" column="item_a" jdbcType="VARCHAR"/>
            <result property="itemB" column="item_b" jdbcType="VARCHAR"/>
            <result property="itemC" column="item_c" jdbcType="VARCHAR"/>
            <result property="itemD" column="item_d" jdbcType="VARCHAR"/>
            <result property="answer" column="answer" jdbcType="VARCHAR"/>
            <result property="analysis" column="analysis" jdbcType="VARCHAR"/>
            <result property="picUrl" column="pic_url" jdbcType="VARCHAR"/>
            <result property="keyWords" column="key_words" jdbcType="VARCHAR"/>
            <result property="star" column="star" jdbcType="TINYINT"/>
            <result property="errRate" column="err_rate" jdbcType="DOUBLE"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,order_id,code,
        type,question,select_item,
        item_a,item_b,item_c,
        item_d,answer,analysis,
        pic_url,key_words,star,
        err_rate
    </sql>
</mapper>
